# Environment variable template for EBAP.
# Copy this file to .env and fill in your values before running docker compose.
#
#   cp .env.example .env

# --- PostgreSQL ---
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin
POSTGRES_DB=ebap_db

# --- MinIO (S3-compatible object storage) ---
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=password

# --- Kafka ---
KAFKA_BROKERS=kafka:9092,kafka-2:9092

# --- Redis ---
REDIS_HOST=redis
REDIS_PORT=6379

# --- Spark ---
SPARK_MASTER=spark://spark-master-streaming:7077
AWS_ACCESS_KEY_ID=admin
AWS_SECRET_ACCESS_KEY=password
AWS_REGION=us-east-1

# --- Iceberg JDBC Catalog ---
ICEBERG_CATALOG_DB=iceberg_catalog
ICEBERG_WAREHOUSE=s3a://ebap-silver/
